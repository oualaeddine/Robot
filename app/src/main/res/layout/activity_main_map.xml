                android:layout_width="match_parent"
                android:layout_height="match_parent" >

        <TextView
                        android:text="Vitesse"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/Vitesse_textView"
            android:textStyle="normal|bold" />

                    <SeekBar
                        style="@style/Widget.AppCompat.SeekBar.Discrete"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:max="4"
                        android:progress="2"
                        android:id="@+id/seekBar2"
                        android:layout_weight="1" />




            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <TextView
                    android:text="Destination:"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:id="@+id/Destination_textView"

                    android:maxLines="1"
                    android:layout_weight="1"
                    android:textStyle="normal|bold" />

                <RadioGroup
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:checkedButton="@+id/Nradio"
                    android:orientation="horizontal"
                    android:layout_weight="2"
                    android:id="@+id/direction">

                    <RadioButton
                        android:text="O"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/Oradio"
                        android:layout_weight="1" />

                    <RadioButton
                        android:text="E"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/Eradio"
                        android:layout_weight="1" />

                    <RadioButton
                        android:text="S"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/Sradio"
                        android:layout_weight="1" />

                    <RadioButton
                        android:text="N"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/Nradio"
                        android:layout_weight="1" />
                </RadioGroup>

            </TableRow>

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:visibility="gone">

                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TextView
                        android:text="Energie:"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/Power_textView" />

                    <ProgressBar
                        style="?android:attr/progressBarStyleHorizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/progressBar2"
                        android:layout_weight="1"
                        android:progress="100"
                        android:max="100" />

                </LinearLayout>

            </TableRow>
        </TableLayout>

        <Button
            android:text="Deplacer"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/buttonDeplacer" />

    </LinearLayout>

</RelativeLayout>
